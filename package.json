{
  "name": "promise-worker-bi",
  "version": "2.0.2",
  "description": "Bidirectional communication with a Web Worker using Promises",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rimraf dist && mkdirp dist && babel index.js --out-file dist/index.js",
    "build-test": "npm run build && node bin/build-worker-bundles.js && babel test -d dist/test",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "eslint *.js",
    "mocha": "npm run build-test && mocha dist/test/test.js",
    "test": "npm run flow && npm run lint && npm run mocha",
    "test-local": "npm run build-test && zuul --local --no-coverage dist/test/test.js",
    "test-browser": "npm run build-test && zuul --no-coverage ./dist/test/test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dumbmatter/promise-worker-bi.git"
  },
  "keywords": [
    "web worker",
    "promise",
    "webworker",
    "worker"
  ],
  "author": "Nolan Lawson <nolan@nolanlawson.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dumbmatter/promise-worker-bi/issues"
  },
  "homepage": "https://github.com/dumbmatter/promise-worker-bi",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.0",
    "browserify": "^14.1.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-plugin-import": "^2.2.0",
    "flow-bin": "^0.41.0",
    "glob-promise": "^3.1.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "pseudo-worker": "^1.1.0",
    "rimraf": "^2.5.3",
    "stream-to-promise": "^1.1.0",
    "zuul": "^3.10.1",
    "zuul-ngrok": "^4.0.0"
  },
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-flow-strip-types"
    ]
  }
}
