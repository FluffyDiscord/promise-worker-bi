{
  "name": "promise-worker-bi",
  "version": "2.2.1",
  "description": "Bidirectional communication with a Web Worker using Promises",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "yarn run build",
    "build": "rimraf dist && mkdirp dist && babel index.js --out-file dist/index.js",
    "build-test": "yarn run build && node bin/build-worker-bundles.js && babel test -d dist/test",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "eslint *.js test/*.js bin/*.js",
    "prettier": "prettier --write '*.js' && prettier --write 'test/*.js' && prettier --write 'bin/*.js'",
    "mocha": "yarn run build-test && mocha dist/test/test.js",
    "test": "yarn run flow && yarn run lint && yarn run mocha",
    "test-local": "yarn run build-test && zuul --local --no-coverage dist/test/test.js",
    "test-browser": "yarn run build-test && zuul --no-coverage ./dist/test/test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dumbmatter/promise-worker-bi.git"
  },
  "keywords": [
    "web worker",
    "promise",
    "webworker",
    "worker"
  ],
  "author": "Nolan Lawson <nolan@nolanlawson.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dumbmatter/promise-worker-bi/issues"
  },
  "homepage": "https://github.com/dumbmatter/promise-worker-bi",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.0",
    "browserify": "^16.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "flow-bin": "^0.69.0",
    "glob": "^7.1.2",
    "glob-promise": "^3.1.0",
    "mkdirp": "^0.5.1",
    "mocha": "^5.1.0",
    "prettier": "^1.12.0",
    "pseudo-worker": "^1.1.0",
    "rimraf": "^2.5.3",
    "stream-to-promise": "^1.1.0",
    "zuul": "^3.10.1"
  },
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-flow-strip-types"
    ]
  }
}
